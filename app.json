{
  "id": "nl.hdg.test",
  "version": "0.0.1",
  "compatibility": ">=1.5.0",
  "sdk": 2,
  "name": {
    "en": "Driver Test"
  },
  "description": {
    "en": "Test app"
  },
  "category": [
    "appliances"
  ],
  "permissions": [
    "homey:manager:speech-output",
    "homey:wireless:433"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "H.T. de Groot",
    "email": "harriedegroot@gmail.com"
  },
  "drivers": [
    {
      "id": "novy-intouch",
      "name": {
        "en": "Novy Intouch",
        "nl": "Novy Intouch"
      },
      "icon": "",
      "class": "other",
      "capabilities": [
        "button",
        "channel_down",
        "channel_up",
        "dim"
      ],
      "images": {
        "large": "./drivers/novy-intouch/assets/images/large.jpg",
        "small": "./drivers/novy-intouch/assets/images/small.jpg"
      },
      "rf": {
        "signal": "intouch"
      },
      "settings": [],
      "pair": [
        {
          "id": "rf.imitate",
          "options": {
            "title": "rf.pair.imitate.title.wall_switch",
            "body": "rf.pair.imitate.body.wall_switch"
          },
          "navigation": {
            "next": "rf.test_switch"
          }
        },
        {
          "id": "rf.test_switch",
          "options": {
            "title": "rf.pair.test.title.wall_switch",
            "body": "rf.pair.test.body.wall_switch"
          },
          "navigation": {
            "prev": "rf.imitate",
            "next": "rf.done"
          }
        },
        {
          "id": "rf.done"
        }
      ]
    }
  ],
  "signals": {
    "433": {
      "intouch": {
        "sof": [
          [
            231
          ]
        ],
        "eof": [],
        "words": [
          [
            404,
            709
          ],
          [
            757,
            356
          ]
        ],
        "interval": 10000,
        "sensitivity": 0.5,
        "repetitions": 20,
        "minimalLength": 12,
        "maximalLength": 18
      }
    }
  }
}